{
    "name": "calendar-module",
    "version": "0.1.0",
    "private": true,
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript",
            "options": {
                "groups": [
                    [
                        "^react",
                        "^react-dom",
                        "^redux",
                        "^@?\\w",
                        "^@portal",
                        "^components",
                        "^services",
                        "^utils",
                        "^constants",
                        "^config",
                        "^interfaces",
                        "^assets",
                        "^pages",
                        "^[./]"
                    ],
                    [
                        "\\u0000"
                    ]
                ]
            }
        }
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.39",
        "@types/react": "^18.2.17",
        "@types/react-dom": "^18.2.7",
        "axios": "^1.4.0",
        "node-sass": "^7.0.3",
        "primeicons": "^6.0.1",
        "primereact": "^9.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.1",
        "react-scripts": "^5.0.1",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@formatjs/cli": "^5.1.0",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.isequal": "^4.5.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.39.0",
        "eslint-plugin-formatjs": "^4.3.1",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "import-sort-style-module": "^6.0.0",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "prettier": "^2.8.8",
        "prettier-plugin-import-sort": "^0.0.7",
        "typescript": "^4.8.4",
        "bootstrap": "^5.1.3"
    },
    "scripts": {
        "start": "react-scripts start --watch",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "format": "prettier --write \"src/**/*.+(ts|tsx|js|jsx|json|css|md)\" --ignore-path .prettierignore"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
